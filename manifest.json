{
  "description": "Show freshness of pages being served up by Varnish cache based on HIT/MISS headers.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/button_gray.png"
  },
  "icons": {
    "16":  "img/icon_small.png",
    "48":  "img/icon_medium.png",
    "128": "img/icon_large.png"
  },
  "manifest_version": 2,
  "name": "Varnish Indicator",
  "permissions": [
    "webRequest",
    "webNavigation",
    "http://*/*",
    "https://*/*"
  ],
  "minimum_chrome_version": "17",
  "version": "0.1.0"
}
